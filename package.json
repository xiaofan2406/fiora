{
  "name": "fiora",
  "version": "1.0.2",
  "author": "Xiaofan Wu (https://github.com/xiaofan2406)",
  "description": "Forms, for react",
  "keywords": [
    "react",
    "forms"
  ],
  "repository": "xiaofan2406/fiora",
  "bugs": {
    "url": "https://github.com/xiaofan2406/fiora/issues"
  },
  "license": "MIT",
  "engines": {
    "node": ">=8"
  },
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "sideEffects": false,
  "scripts": {
    "precommit": "lint-staged",
    "lint:eslint": "eslint '**/*.{js,flow}' --ignore-path .gitignore",
    "lint:prettier": "prettier-check '**/*.{js,json,flow,md}' --ignore-path .gitignore",
    "lint": "yarn lint:eslint && yarn lint:prettier",
    "format": "prettier '**/*.{js,json,flow,md}' --write  --ignore-path .gitignore",
    "test": "NODE_ENV=test jest --watch --updateSnapshot",
    "coverage": "NODE_ENV=test jest --coverage",
    "prebuild": "rm -rf dist",
    "build:cjs": "NODE_ENV=production rollup -c",
    "build:esm": "NODE_ENV=production babel src --out-dir dist/esm",
    "build": "yarn build:cjs && yarn build:esm",
    "prepublishOnly": "yarn build",
    "postpublish": "git push && git push --tags",
    "docs:dev": "cd ./www && yarn dev",
    "docs:build": "cd ./www && yarn build",
    "predocs:release": "yarn docs:build",
    "docs:release": "surge -p ./www/public -d fiora.surge.sh"
  },
  "lint-staged": {
    "*.{js,json,flow,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "prettier": {
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "browserslist": [
    "Chrome >= 60",
    "Safari >= 10.1",
    "iOS >= 10.3",
    "Firefox >= 54",
    "Edge >= 15"
  ],
  "peerDependencies": {
    "react": "^16.4.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0",
    "@babel/core": "^7.0.0",
    "@babel/plugin-proposal-class-properties": "^7.0.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/preset-flow": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "^9.0.0",
    "babel-jest": "^23.4.2",
    "eslint": "^5.5.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^3.0.1",
    "eslint-plugin-flowtype": "^2.50.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-react": "^7.11.1",
    "flow-bin": "^0.80.0",
    "husky": "^0.14.3",
    "jest": "^23.5.0",
    "jest-dom": "^1.12.0",
    "lint-staged": "^7.2.2",
    "prettier": "^1.14.2",
    "prettier-check": "^2.0.0",
    "react": "^16.4.2",
    "react-dom": "^16.4.2",
    "react-testing-library": "^5.0.1",
    "rollup": "^0.65.2",
    "rollup-plugin-babel": "^4.0.3",
    "rollup-plugin-commonjs": "^9.1.6",
    "rollup-plugin-node-resolve": "^3.4.0",
    "rollup-plugin-replace": "^2.0.0",
    "rollup-plugin-terser": "^2.0.2",
    "surge": "^0.20.1"
  }
}
