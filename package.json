{
  "name": "fiora",
  "version": "1.0.3",
  "author": "Xiaofan Wu (https://github.com/xiaofan2406)",
  "description": "Forms, for react",
  "keywords": [
    "react",
    "forms"
  ],
  "repository": "xiaofan2406/fiora",
  "bugs": {
    "url": "https://github.com/xiaofan2406/fiora/issues"
  },
  "license": "MIT",
  "engines": {
    "node": ">=8"
  },
  "main": "dist/cjs/fiora.js",
  "module": "dist/esm/index.js",
  "browser": "dist/umd/fiora.min.js",
  "typings": "./fiora.d.ts",
  "sideEffects": false,
  "scripts": {
    "typecheck": "tsc --noEmit",
    "precommit": "lint-staged",
    "lint:tslint": "tslint 'src/**/*.{ts,tsx}' './fiora.d.ts' -c tslint.json",
    "lint:eslint": "eslint '**/*.js' --ignore-path .gitignore",
    "lint:prettier": "prettier-check '**/*.{js,json,ts,tsx,md}' --ignore-path .gitignore",
    "lint": "yarn lint:tslint && yarn lint:eslint && yarn lint:prettier",
    "format": "prettier '**/*.{js,json,ts,tsx,md}' --write --ignore-path .gitignore",
    "test": "NODE_ENV=test jest --watch --updateSnapshot",
    "coverage": "NODE_ENV=test jest --coverage",
    "prebuild": "rm -rf dist",
    "build:cjs": "NODE_ENV=production rollup -c",
    "build:esm": "NODE_ENV=production babel src --out-dir dist/esm --extensions '.ts,.tsx'",
    "build": "yarn build:cjs && yarn build:esm",
    "prepublishOnly": "yarn build",
    "postpublish": "git push && git push --tags",
    "docs:dev": "cd ./www && yarn dev",
    "docs:build": "cd ./www && yarn build",
    "predocs:release": "yarn docs:build",
    "docs:release": "surge -p ./www/public -d fiora.surge.sh"
  },
  "lint-staged": {
    "*.{js,json,ts,tsx,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "prettier": {
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "browserslist": [
    "Chrome >= 60",
    "Safari >= 10.1",
    "iOS >= 10.3",
    "Firefox >= 54",
    "Edge >= 15"
  ],
  "peerDependencies": {
    "react": "^16.4.2"
  },
  "dependencies": {},
  "devDependencies": {
    "@babel/cli": "^7.1.0",
    "@babel/core": "^7.1.0",
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0",
    "@babel/preset-env": "^7.1.0",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-typescript": "^7.1.0",
    "@types/react": "^16.4.14",
    "@types/react-dom": "^16.0.8",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "^9.0.0",
    "babel-jest": "^23.6.0",
    "eslint": "^5.6.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^3.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-react": "^7.11.1",
    "husky": "^0.14.3",
    "jest": "^23.6.0",
    "jest-dom": "^1.12.1",
    "lint-staged": "^7.3.0",
    "prettier": "^1.14.3",
    "prettier-check": "^2.0.0",
    "react": "^16.5.2",
    "react-dom": "^16.5.2",
    "react-testing-library": "^5.1.0",
    "rollup": "^0.66.2",
    "rollup-plugin-babel": "^4.0.3",
    "rollup-plugin-commonjs": "^9.1.8",
    "rollup-plugin-node-resolve": "^3.4.0",
    "rollup-plugin-replace": "^2.0.0",
    "rollup-plugin-terser": "^3.0.0",
    "rollup-plugin-typescript": "^1.0.0",
    "surge": "^0.20.1",
    "tslint": "^5.11.0",
    "tslint-config-prettier": "^1.15.0",
    "typescript": "^3.1.1"
  }
}
