{
  "name": "fiora",
  "version": "0.0.20",
  "author": "Xiaofan Wu (https://github.com/xiaofan2406)",
  "description": "Forms, with react + redux",
  "keywords": ["react", "redux", "forms"],
  "repository": "xiaofan2406/fiora",
  "bugs": {
    "url": "https://github.com/xiaofan2406/fiora/issues"
  },
  "license": "MIT",
  "engines": {
    "node": ">=8"
  },
  "main": "lib/index.js",
  "scripts": {
    "precommit": "lint-staged",
    "lint:eslint": "eslint src config docs/src test",
    "lint:prettier":
      "prettier-check '{src,docs/src,config,test}/**/*.{js,json,css,md}'",
    "lint": "npm run lint:eslint && npm run lint:prettier",
    "format":
      "prettier '{src,docs/src,config,test}/**/*.{js,json,css,md}' --write",
    "prebuild": "npm run lint && rm -rf ./lib",
    "build": "NODE_ENV=production babel src --out-dir lib",
    "test": "NODE_ENV=test jest --watch",
    "coverage": "NODE_ENV=test jest --coverage",
    "prepublishOnly": "NODE_ENV=production npm run build",
    "postpublish": "git push && git push --tags",
    "dev":
      "NODE_ENV=development webpack-dev-server --config ./docs/config/webpack.dev.js",
    "build:docs":
      "rm -rf ./docs/dist && NODE_ENV=production webpack --config ./docs/config/webpack.prod.js",
    "predocs":
      "rm -rf ./docs/dist && NODE_ENV=production webpack --config ./docs/config/webpack.prod.js",
    "docs": "surge -p ./docs/dist -d fiora.surge.sh"
  },
  "lint-staged": {
    "*.{js,json,css,md}": ["prettier --write", "git add"]
  },
  "prettier": {
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "jest": {
    "preset": "<rootDir>/config/jest.config.json"
  },
  "browserslist": [
    "Chrome >= 60",
    "Safari >= 10.1",
    "iOS >= 10.3",
    "Firefox >= 54",
    "Edge >= 15"
  ],
  "dependencies": {
    "prop-types": "^15.6.0"
  },
  "peerDependencies": {
    "react": "^15 || ^16",
    "react-redux": "^5",
    "redux": "^3"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0-beta.40",
    "@babel/core": "^7.0.0-beta.40",
    "@babel/plugin-proposal-class-properties": "^7.0.0-beta.40",
    "@babel/plugin-proposal-export-default-from": "^7.0.0-beta.40",
    "@babel/plugin-proposal-export-namespace-from": "^7.0.0-beta.40",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0-beta.40",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0-beta.40",
    "@babel/preset-env": "^7.0.0-beta.40",
    "@babel/preset-react": "^7.0.0-beta.40",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^8.2.2",
    "babel-jest": "^22.4.1",
    "babel-loader": "^8.0.0-beta.0",
    "babel-plugin-emotion": "^9.0.1",
    "css-loader": "^0.28.10",
    "emotion": "^9.0.2",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint": "^4.18.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-import-resolver-webpack": "^0.8.4",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-import": "^2.9.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.7.0",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^1.1.9",
    "filemanager-webpack-plugin": "^1.0.24",
    "html-webpack-plugin": "^2.30.1",
    "husky": "^0.14.3",
    "jest": "^22.4.2",
    "lint-staged": "^7.0.0",
    "prettier": "^1.10.2",
    "prettier-check": "^2.0.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-emotion": "^9.0.2",
    "react-hot-loader": "^4.0.0-rc.0",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.2.2",
    "react-test-renderer": "^16.2.0",
    "redux": "^3.7.2",
    "redux-immutable-state-invariant": "^2.1.0",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.2.0",
    "serve": "^6.4.11",
    "style-loader": "^0.20.2",
    "surge": "^0.19.0",
    "uglifyjs-webpack-plugin": "^1.2.2",
    "url-loader": "^0.6.2",
    "webpack": "^3.11.0",
    "webpack-dev-server": "^2.11.1",
    "webpack-manifest-plugin": "^1.3.2"
  }
}
