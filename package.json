{
  "name": "fiora",
  "version": "1.0.1",
  "author": "Xiaofan Wu (https://github.com/xiaofan2406)",
  "description": "Forms, for react",
  "keywords": [
    "react",
    "forms"
  ],
  "repository": "xiaofan2406/fiora",
  "bugs": {
    "url": "https://github.com/xiaofan2406/fiora/issues"
  },
  "license": "MIT",
  "engines": {
    "node": ">=8"
  },
  "main": "dist/commonjs",
  "module": "dist/es6",
  "browser": "dist/umd/fiora-umd.min.js",
  "sideEffects": false,
  "scripts": {
    "precommit": "lint-staged",
    "lint:eslint": "eslint config src test",
    "lint:prettier": "prettier-check '{config,src,test}/**/*.{js,mjs,json,md}'",
    "lint": "yarn lint:eslint && yarn lint:prettier",
    "format": "prettier '{config,src,test}/**/*.{js,mjs,json,md}' --write",
    "test": "NODE_ENV=test jest --watch --updateSnapshot",
    "coverage": "NODE_ENV=test jest --coverage",
    "build:commonjs": "NODE_ENV=production babel src --out-dir dist/commonjs",
    "build:es6": "NODE_ENV=production MODULE=es6 babel src --out-dir dist/es6",
    "build:umd": "NODE_ENV=production webpack --config ./config/webpack.dist.js",
    "build": "yarn build:commonjs && yarn build:es6 && yarn build:umd",
    "prepublishOnly": "yarn build",
    "postpublish": "git push && git push --tags"
  },
  "lint-staged": {
    "*.{js,mjs,json,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "prettier": {
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "browserslist": [
    "Chrome >= 60",
    "Safari >= 10.1",
    "iOS >= 10.3",
    "Firefox >= 54",
    "Edge >= 15"
  ],
  "peerDependencies": {
    "react": "^16.4.2"
  },
  "dependencies": {},
  "devDependencies": {
    "@babel/cli": "^7.0.0",
    "@babel/core": "^7.0.0",
    "@babel/plugin-proposal-class-properties": "^7.0.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/preset-flow": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "^9.0.0",
    "babel-jest": "^23.4.2",
    "babel-loader": "^8.0.1",
    "eslint": "^5.5.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^3.0.1",
    "eslint-loader": "^2.1.0",
    "eslint-plugin-flowtype": "^2.50.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-react": "^7.11.1",
    "flow-bin": "^0.80.0",
    "husky": "^0.14.3",
    "jest": "^23.5.0",
    "jest-dom": "^1.12.0",
    "lint-staged": "^7.2.2",
    "prettier": "^1.14.2",
    "prettier-check": "^2.0.0",
    "react": "^16.4.2",
    "react-dom": "^16.4.2",
    "react-testing-library": "^5.0.0",
    "uglifyjs-webpack-plugin": "^1.3.0",
    "webpack": "^4.17.1",
    "webpack-cli": "^3.1.0"
  }
}
