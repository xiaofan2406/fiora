[ignore]
.*/config/.*
.*/coverage/.*
.*/dist/.*
.*/node_modules/.*
.*/test/.*

[include]

[libs]
./src/types.js

[lints]

[options]
module.file_ext=.js
module.file_ext=.mjs
module.file_ext=.json
module.use_strict=true
experimental.const_params=true
suppress_comment= \\(.\\|\n\\)*\\$FlowFixMe
module.name_mapper='^assets' -> '<PROJECT_ROOT>/docs/assets'
module.name_mapper='^components' -> '<PROJECT_ROOT>/docs/components'
module.name_mapper='^factories' -> '<PROJECT_ROOT>/docs/factories'
module.name_mapper='^forms' -> '<PROJECT_ROOT>/docs/forms'
module.name_mapper='^fiora' -> '<PROJECT_ROOT>/src'
module.name_mapper='^store' -> '<PROJECT_ROOT>/docs/store'
module.name_mapper='^styles' -> '<PROJECT_ROOT>/docs/styles'
module.name_mapper='^utils' -> '<PROJECT_ROOT>/docs/utils'
module.name_mapper='^widgets' -> '<PROJECT_ROOT>/docs/widgets'
